<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Records</title>
</head>
<body>
    <h1>Records</h1>
    <ul>
        <% records %>
        <% records.forEach(function(record) { %>
            <li><%= record.Driver_FirstName %> - <%= record.Driver_LastName %> - <%= record.RegistrationNumber %></li>
        <% }); %>
    </ul>
</body>
</html>
